<view bind:__l="__l"><view class="user"><view class="left"><image src="{{userInfo.avatarUrl}}" data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" bindtap="__e"></image></view><view class="right"><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="username" bindtap="__e">{{userInfo.nickname}}</view><view data-event-opts="{{[['tap',[['toSetting',['$event']]]]]}}" class="signature" bindtap="__e">{{userInfo.email==null?'点击完善账户设置':userInfo.email}}</view></view><view data-event-opts="{{[['tap',[['toMyQR',['$event']]]]]}}" class="qr-code" bindtap="__e"><view class="icon pin-icon">share</view></view></view><view class="VIP animation-scale-down"><view class="img"><image src="../../static/img/VIP.png"></image></view><view class="title">Pin Premium 黑金会员</view><view class="tis">查看 Premium 特权</view></view><view class="order"><view class="list"><block wx:for="{{orderList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toOrderList',['$0'],[[['orderList','',index,'key']]]]]]]}}" class="box" bindtap="__e"><view class="img"><view class="icon pin-icon">{{''+row.icon}}</view></view><view class="text">{{row.text}}</view></view></block></view><view class="balance-info"><view class="left"><view data-event-opts="{{[['tap',[['toCheckIn']]]]}}" class="box" bindtap="__e"><view class="num">{{hasCheckedIn?'已签到':'未签到'}}</view><view class="text">签到赢积分</view></view><view class="box"><view class="num">{{userInfo.credit}}</view><view class="text">积分</view></view><view class="box"><view class="num">{{userInfo.balance}}</view><view class="text">余额</view></view></view><view class="right"><view data-event-opts="{{[['tap',[['toDeposit',['$event']]]]]}}" class="box" bindtap="__e"><view class="img"><view class="icon pin-icon">monetization_on</view></view><view class="text">充值</view></view></view></view></view><view class="toolbar"><view class="title"><view class="pin-icon _i">style</view>我的工具栏</view><view class="list"><block wx:for="{{mytoolbarList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['mytoolbarList','',index,'url']]]]]]]}}" class="box" bindtap="__e"><view class="icon"><view class="pin-icon _i">{{row.icon}}</view></view><view class="text">{{row.text}}</view></view></block></view></view><view class="place-bottom"></view></view>