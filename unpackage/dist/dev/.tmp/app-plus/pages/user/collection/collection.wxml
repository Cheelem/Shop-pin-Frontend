<view bind:__l="__l"><view class="tabr" style="{{'top:'+(headerTop)+';'}}"><view data-event-opts="{{[['tap',[['switchType',['product']]]]]}}" class="{{[(typeClass=='product')?'on':'']}}" bindtap="__e">{{"商品("+productList.length+")"}}</view><view data-event-opts="{{[['tap',[['switchType',['store']]]]]}}" class="{{[(typeClass=='store')?'on':'']}}" bindtap="__e">{{"店铺("+storeList.length+")"}}</view><view class="{{['border',typeClass]}}"></view></view><view class="place"></view><view class="list"><view class="{{['sub-list product',subState]}}"><block wx:if="{{productList.length==0}}"><view class="tis"><full-page-empty-state title="收藏列表为空" description="到处逛逛并把商品加入收藏吧"></full-page-empty-state></view></block><block wx:for="{{productList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row pin-card"><view data-event-opts="{{[['tap',[['deleteCoupon',['$0','$1'],[[['productList','',index,'id']],'productList']]]]]}}" class="menu" catchtap="__e"><view class="icon pin-icon">delete</view></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',typeClass=='product'?theIndex==index?'open':oldIndex==index?'close':'':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['toproduct',['$0'],[[['productList','',index]]]]]]]}}" class="product-info" bindtap="__e"><view class="img"><image src="{{row.product.imageUrls}}"></image></view><view class="info"><view class="title">{{row.product.name}}</view><view class="price-number"><view class="keep-num">905人收藏</view><view class="price">{{"￥"+row.product.price}}</view></view></view></view></view></view></block></view><view class="{{['sub-list store',subState]}}"><block wx:if="{{storeList.length==0}}"><view class="tis">没有数据~</view></block><block wx:for="{{storeList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row pin-card"><view data-event-opts="{{[['tap',[['deleteCoupon',['$0','$1'],[[['storeList','',index,'id']],'storeList']]]]]}}" class="menu" catchtap="__e"><view class="icon pin-icon">delete</view></view><view data-event-opts="{{[['touchstart',[['touchStart',[index,'$event']]]],['touchmove',[['touchMove',[index,'$event']]]],['touchend',[['touchEnd',[index,'$event']]]]]}}" class="{{['carrier',typeClass=='store'?theIndex==index?'open':oldIndex==index?'close':'':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view class="left"><image src="{{row.store.logoUrl}}"></image></view><view class="right"><view>{{''+row.store.name+''}}</view></view></view></view></block></view></view></view>